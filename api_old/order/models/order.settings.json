{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store": {
      "via": "orders",
      "model": "store"
    },
    "customer": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "delivery_date": {
      "type": "datetime"
    },
    "name": {
      "type": "string"
    },
    "deliver_types": {
      "collection": "deliver-type"
    },
    "Weekly_needed": {
      "type": "boolean",
      "default": false
    },
    "total_amount": {
      "type": "float"
    },
    "disocunt": {
      "type": "float"
    },
    "order_details": {
      "via": "orders",
      "collection": "order-detail",
      "dominant": true
    },
    "discount_code": {
      "type": "string"
    },
    "quantity": {
      "type": "biginteger"
    },
    "order_statuses": {
      "collection": "order-status"
    },
    "order_histories": {
      "via": "order",
      "collection": "order-history"
    }
  }
}
