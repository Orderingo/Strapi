{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address",
    "name": "address"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "city": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::city.city"
    },
    "state": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::state.state"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "address_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address-type.address-type"
    },
    "streetaddress": {
      "type": "string",
      "required": true
    },
    "userAddresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "addresses"
    }
  }
}
