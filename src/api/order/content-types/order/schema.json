{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "orders"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "delivery_date": {
      "type": "datetime"
    },
    "name": {
      "type": "string"
    },
    "deliver_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deliver-type.deliver-type"
    },
    "Weekly_needed": {
      "type": "boolean",
      "default": false
    },
    "total_amount": {
      "type": "float"
    },
    "disocunt": {
      "type": "float"
    },
    "order_details": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order-detail.order-detail",
      "inversedBy": "orders"
    },
    "discount_code": {
      "type": "string"
    },
    "quantity": {
      "type": "biginteger"
    },
    "order_statuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-status.order-status"
    },
    "order_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-history.order-history",
      "mappedBy": "order"
    }
  }
}
