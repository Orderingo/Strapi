{
  "kind": "collectionType",
  "collectionName": "product_sizes",
  "info": {
    "singularName": "product-size",
    "pluralName": "product-sizes",
    "displayName": "Product-size",
    "name": "product-size"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "decimal"
    },
    "discount_type": {
      "type": "string"
    },
    "discount": {
      "type": "float"
    },
    "promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::promotion.promotion",
      "mappedBy": "product_size_price"
    }
  }
}
