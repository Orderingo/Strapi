{
  "kind": "collectionType",
  "collectionName": "order_details",
  "info": {
    "singularName": "order-detail",
    "pluralName": "order-details",
    "displayName": "Order-detail",
    "name": "order-detail"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "isOffer": {
      "type": "boolean",
      "default": false
    },
    "Amount": {
      "type": "string",
      "required": true
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "order_details"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "orderDetail"
    }
  }
}
