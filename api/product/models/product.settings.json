{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "stores": {
      "via": "products",
      "collection": "store",
      "dominant": true
    },
    "Ingredients": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "productCategories": {
      "via": "products",
      "collection": "menu-category",
      "dominant": true
    },
    "Price": {
      "required": true,
      "type": "decimal"
    },
    "preferredCategories": {
      "via": "products",
      "collection": "preferred-category",
      "dominant": true
    },
    "createdDate": {
      "type": "datetime"
    },
    "createdBy": {
      "type": "string"
    },
    "orderDetail": {
      "model": "order-detail",
      "via": "products"
    },
    "itemCode": {
      "type": "string"
    },
    "productProperty1": {
      "collection": "product-properties"
    },
    "delivery_days": {
      "collection": "delivery-days"
    },
    "allergen": {
      "type": "string"
    },
    "productimagee": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "availableForDailyMenu": {
      "type": "boolean",
      "default": false
    },
    "ProductCategory": {
      "type": "string"
    },
    "PrefferedCategory": {
      "type": "string"
    },
    "DeliveryDays": {
      "type": "string"
    },
    "ProductProperties": {
      "type": "string"
    },
    "Packaging": {
      "type": "string"
    },
    "UserId": {
      "type": "biginteger"
    }
  }
}
