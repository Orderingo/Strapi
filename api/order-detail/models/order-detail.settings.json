{
  "kind": "collectionType",
  "collectionName": "order_details",
  "info": {
    "name": "order_detail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "offers": {
      "collection": "offer",
      "via": "order_detail"
    },
    "isOffer": {
      "type": "boolean",
      "default": false
    },
    "Amount": {
      "type": "string",
      "required": true
    },
    "orders": {
      "via": "order_details",
      "collection": "order"
    },
    "products": {
      "via": "orderDetail",
      "collection": "product"
    }
  }
}
